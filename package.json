{
  "name": "react-airfieldhub",
  "version": "0.1.4",
  "main": "dist/index.js",
  "module": "dist/react-airfieldhub.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/react-airfieldhub.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-airfieldhub.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/create-react-app": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "css-loader": "^6.7.1",
    "dts-cli": "^1.6.0",
    "dts-css-modules-loader": "^2.0.1",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "iframe-resizer-react": "^1.1.0"
  },
  "author": {
    "name": "Airfield Hub",
    "email": "hello@airfieldhub.com",
    "url": "https://airfieldhub.com"
  },
  "keywords": [
    "airfieldhub",
    "airfield hub",
    "react-airfieldhub",
    "airfield hub react",
    "simple ppr",
    "ppr forms",
    "aviation"
  ],
  "homepage": "https://github.com/airfieldhub/react-airfieldhub",
  "license": "BSD-4-Clause"
}
